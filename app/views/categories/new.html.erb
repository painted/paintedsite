
<%= form_for @category, html: { multipart: true } do |f| %>

	<%= f.label :title %><br>
	<%= f.text_field :title %><br>

	<%= f.label :description %><br>
	<%= f.text_area :description %><br>

	<%= f.label :image %><br>
	<%= f.file_field :image %><br>

	<%= f.label :url %><br>
	<%= f.url_field :url %><br>

	<%= f.label :tag_names, 'Tags' %><br>
	<%= f.text_field :tag_names %><br>

	<%= f.submit 'Submit' %>

<% end %>
